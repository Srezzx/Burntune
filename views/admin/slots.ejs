          <div class="container-fluid">
            <h3 class="page-title">All Slots</h3>
            <div class="row">
              <div class="col-md-12">
                <!-- BORDERED TABLE -->
                <div
                  style="border: none; background-color: #f3f5f8"
                  class="panel"
                >
                  <div class="panel-heading">
                    <!-- <h3 class="panel-title">Bordered Table</h3> -->
                  </div>
                  <div class="panel-body">
                    <table class="table table-bordered">
                      <thead>
                        <tr>
                          <th>SLNO</th>
                          <th>Date</th>
                          <th>Start</th>
                          <th>End</th>
                          <th>instrument</th>
                          <th>Students Cap.</th>
                          <th>Seats left</th>
                          <th>Schedule</th>
                          <!-- <th>EDIT</th>
												<th>DELETE</th> -->
                        </tr>
                      </thead>
                      <tbody>
                        <% var l = allslots.length; for(var i=1;i<= l;i++){ %>
                        <tr>
                          <td style="padding: 4px">
                            <a href="#"><%= l-i %></a>
                          </td>
                          <td style="padding: 4px">
                            <%= allslots[l-i].date %>
                          </td>
                          <td style="padding: 4px">
                            <%= allslots[l-i].time_from %>
                          </td>
                          <td style="padding: 4px">
                            <%= allslots[l-i].time_to %>
                          </td>
                          <td style="padding: 4px">
                            <%= allslots[l-i].instrument %>
                          </td>
                          <td style="padding: 4px">
                            <%= allslots[l-i].limit %>
                          </td>
                          <td style="padding: 4px">
                            <%= allslots[l-i].limit -
                            allslots[l-i].students.length %>
                          </td>
                          <!-- <td style="padding:4px;"><a class="btn btn-sm btn-warning" href="/scheduleslots/<%= allslots[l-i]._id %>">Schedule</a></td>
												<td style="padding:4px;"><a class="btn btn-sm btn-warning" href="/slots/edit/<%= allslots[l-i]._id %>">EDIT</a></td>
												<td style="padding:4px;"><a class="btn btn-sm btn-warning" href="/slots/delete/<%= allslots[l-i]._id %>">DELETE</a></td> -->
                          <td style="padding: 4px">
                            <div
                              class="btn-group"
                              role="group"
                              aria-label="Basic example"
                            >
                              <a
                                class="btn btn-sm btn-primary"
                                href="/scheduleslots/<%= allslots[l-i]._id %>"
                                >Schedule</a
                              >
                              <a
                                class="btn btn-sm btn-warning"
                                href="/slots/edit/<%= allslots[l-i]._id %>"
                                >EDIT</a
                              >
                              <form
                                style="display: inline; margin: 0; padding: 0"
                                action="/slots/delete/<%= allslots[l-i]._id %>"
                                method="POST"
                              >
                                <button
                                  class="btn btn-sm btn-danger"
                                  type="submit"
                                >
                                  DELETE
                                </button>
                              </form>
                            </div>
                          </td>
                        </tr>
                        <tr>
                          <td colspan="10">
                            <select style="color: rgb(158, 150, 137);background-color: rgb(24, 26, 27);"class="form-control input-sm">
                              <!-- <option value="students" selected disabled>Students</option> -->
                              <% if(allslots[l-i].students.length <= 0 ) { %> 
								<option hidden value="null">NO STUDENT ALLOTED</option>
							  <% } %> 
							  <% allslots[l-i].students.forEach(function(student){ %>
                              <option value="<%= student.name %>"><%= student.name %></option>
                              <% }) %>
                            </select>
                          </td>
						  </tr>
                        </tr>
                        <% } %>
                      </tbody>
                    </table>
                  </div>
                </div>
                <!-- END BORDERED TABLE -->
